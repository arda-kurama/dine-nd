{
    "expo": {
        "name": "DineND",
        "slug": "dine-nd",
        "version": "1.0.5",
        "runtimeVersion": "1.0.5",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#0C2340"
        },
        "ios": {
            "bundleIdentifier": "com.ardakurama.dinend",
            "supportsTablet": true,
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false
            }
        },
        "android": {
            "package": "com.ardakurama.dinend",
            "icon": "./assets/adaptive-icon.png",
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundImage": "./assets/navy-background.png"
            },
            "edgeToEdgeEnabled": true
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["expo-asset"],
        "owner": "arda-kurama",
        "extra": {
            "eas": {
                "projectId": "dbbb905d-498d-44ed-ade2-680ec0f30fb8"
            }
        }
    }
}
