{
    "cli": {
        "version": ">= 6.3.1",
        "appVersionSource": "remote"
    },
    "build": {
        "production": {
            "channel": "main",
            "autoIncrement": true,
            "android": {
                "buildType": "app-bundle"
            },
            "ios": {
                "distribution": "store"
            }
        },
        "staging": {
            "channel": "staging",
            "autoIncrement": true,
            "android": {
                "buildType": "app-bundle"
            },
            "ios": {
                "distribution": "internal"
            }
        }
    },
    "submit": {
        "production": {
            "android": {
                "track": "internal"
            },
            "ios": {
                "ascAppId": "6451234567"
            }
        }
    }
}
